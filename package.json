{
  "name": "graphql-inspector",
  "private": true,
  "version": "0.5.1",
  "description": "Tooling for GraphQL. Compare GraphQL Schemas, check documents, find breaking changes, find similar types.",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "tools",
    "cli"
  ],
  "sideEffects": false,
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector"
  },
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "format": "prettier --config .prettierrc --write \"packages/*/{src,__tests__}/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "lint-staged": "8.0.4",
    "prettier": "1.15.2"
  },
  "dependencies": {
    "lerna": "3.4.3"
  },
  "workspaces": {
    "packages": [
      "packages/core",
      "packages/cli"
    ]
  }
}
