    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "prettier": "1.15.2",
    "ts-jest": "23.10.4",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "chalk": "2.4.1",
