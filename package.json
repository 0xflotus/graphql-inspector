    "graphql",
    "tools",
    "cli"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "bin": {
    "graphql-diff": "dist/cli.js"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-schema-comparator"
  },
  "scripts": {
